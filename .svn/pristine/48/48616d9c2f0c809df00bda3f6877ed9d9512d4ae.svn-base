<div class="ui-g-12" style="height: 60px; background: #FFF; text-align: center; background: #659b39">
    <img style="max-width: 50px; max-height: 50px;border-radius: 6px;" src="https://s3-us-west-2.amazonaws.com/dapesweb/dapes_verde.jpg">
</div>
<body class="login-body">
    
    <div id="login-wrapper" style="margin-top: 20px;">
        <div id="login-container">
            <span class="title">LOGIN</span>

            <div class="ui-g ui-fluid">
                <!--<div class="ui-g-12">
                     <img style="max-width: 50px; max-height: 50px;border-radius: 6px;" src="https://s3-us-west-2.amazonaws.com/dapesweb/dapes_verde.jpg">
                </div>-->
                <div class="ui-g-12">
                    <input type="text" name="username" [(ngModel)]="user.username" #username="ngModel" required autocomplete="off" class="ui-inputtext ui-corner-all ui-state-default ui-widget" placeholder="Username" />
                </div>
                <div class="ui-g-12">
                    <input type="password" name="password" [(ngModel)]="user.password" #password="ngModel" required autocomplete="off" class="ui-inputtext ui-corner-all ui-state-default ui-widget" placeholder="Password" />
                </div>
                <!--<div class="ui-g-12 ui-md-6">
                    <span class="remember-me">Remember Me</span>
                    <input type="checkbox">
                </div>-->
                <!--<div class="ui-chkbox ui-widget">
                    <div class="ui-helper-hidden-accessible"><input id="j_idt64:0" name="j_idt64" type="checkbox" value="Xbox One"></div>
                    <div class="ui-chkbox-box ui-widget ui-corner-all ui-state-default"><span class="ui-chkbox-icon ui-icon ui-c ui-icon-blank"></span></div>
                </div>-->
                <div class="ui-g-12" style="text-align: center;">
                    <a href="#">Forgot Password</a>
                </div>
                <div class="ui-g-12">
                    <button type="button" class="ui-button ui-widget ui-state-default green-button ui-corner-all ui-button-text-icon-left">
                        <span class="ui-button-icon-left ui-c fa fa-fw ui-icon-person"></span>
                        <span class="ui-button-text ui-c">Entrar</span>
                    </button>
                </div>
                <div class="ui-g-12">
                    <button type="button" class="ui-button ui-widget ui-state-default ui-corner-all ui-button-text-icon-left">
                        <span class="ui-button-icon-left ui-c fa fa-fw ui-icon-person"></span>
                        <span class="ui-button-text ui-c">Cadastrar</span>
                    </button>
                </div>
            </div>
        </div>

        <div class="login-footer"></div>
    </div>
</body>